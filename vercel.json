{
    "version": 2,
    "builds": [
      {
        "src": "backend/index.js",
        "use": "@now/node"
      }
    ],
    "routes": [
      { "src": "/Api/(.*)", "dest": "backend/Api.js" },
      { "src": "/PaymentApi/(.*)", "dest": "backend/PaymentApi.js" },
      { "src": "/(.*)", "dest": "backend/Index.js" }
    ]
  }
  